class Task extends Thread {
    String taskName;

    Task(String name) {
        this.taskName = name;
    }

    public void run() {
        for (int i = 1; i <= 5; i++) {
            System.out.println(taskName + " running: " + i);
        }
    }
}

public class ConcurrentProcessingSystem {
    public static void main(String[] args) {
        Task t1 = new Task("Task-1");
        Task t2 = new Task("Task-2");
        Task t3 = new Task("Task-3");

        t1.start();
