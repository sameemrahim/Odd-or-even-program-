class Employee {
    void work() {
        System.out.println("Employee is working");
    }
}

class Manager extends Employee {
    void manage() {
        System.out.println("Manager is managing team");
    }
}

public class Main {
    public static void main(String[] args) {
        Manager obj = new Manager();
        obj.work();
        obj.manage();
    }
}
