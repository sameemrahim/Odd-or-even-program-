class Vehicle {
    void run() {
        System.out.println("Vehicle is running");
    }
}

class Car extends Vehicle {
    @Override
    void run() {
        System.out.println("Car is running fast");
    }
}

public class Main {
    public static void main(String[] args) {
        Vehicle v = new Car();   // Base reference, Child object
        v.run();
    }
}
