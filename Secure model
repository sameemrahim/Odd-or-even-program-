class Account {
    private double balance;

    public void setBalance(double b) {
        balance = b;
    }

    public double getBalance() {
        return balance;
    }
}

public class Main {
    public static void main(String[] args) {
        Account obj = new Account();
        obj.setBalance(5000);
        System.out.println("Balance: " + obj.getBalance());
    }
}
